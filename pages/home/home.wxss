.topswp{
    height: 320rpx;
}
.topswp image{
    width: 100%;
    height: 100%;
}
.spLis{
    display: flex;
    flex-wrap: wrap;
    border-top: 1px solid #eee;
    border-left: 1px solid #eee;

}
.spLis .spItem{
    box-sizing: border-box;
    width: 33.33%;
    height: 200rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-right: 1px solid #eee;
    border-bottom: 1px solid #eee;
}
.spLis .spItem image{
    width: 80rpx;height: 80rpx;
    margin: 20rpx 0;
}
.newImg{
    display: flex;
    justify-content: space-around;
    padding-top: 30rpx;
}
.newImg image{
    width: 45%;
}